  "root": "libs/joke-master-application",
  "sourceRoot": "libs/joke-master-application/src",
  "prefix": "joke-master",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/joke-master-application"],
